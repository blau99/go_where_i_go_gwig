<div class="page-header">
  <h3>Points Of Interests Photo #<%= @points_of_interests_photo.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Points of interest</dt>
      <dd><%= @points_of_interests_photo.points_of_interest_id %></dd>

      <dt>User</dt>
      <dd><%= @points_of_interests_photo.user_id %></dd>

      <dt>Caption</dt>
      <dd><%= @points_of_interests_photo.caption %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/points_of_interests_photos" class="btn btn-primary">
        Back
      </a>
      <a href="/points_of_interests_photos/<%= @points_of_interests_photo.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_points_of_interests_photo/<%= @points_of_interests_photo.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
