<div class="page-header">
  <h3>Restaurant Favorite #<%= @restaurant_favorite.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Restaurant</dt>
      <dd><%= @restaurant_favorite.restaurant_id %></dd>

      <dt>User</dt>
      <dd><%= @restaurant_favorite.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/restaurant_favorites" class="btn btn-primary">
        Back
      </a>
      <a href="/restaurant_favorites/<%= @restaurant_favorite.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_restaurant_favorite/<%= @restaurant_favorite.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
