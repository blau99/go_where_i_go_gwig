<div class="page-header">
  <h1>
    Accomodation photos
    <a href="/accomodation_photos/new" class="btn btn-lg btn-success">New Accomodation photo</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Accommodation</th>
        <th>User</th>
        <th>Caption</th>
        <th>Actions</th>
      </tr>

      <% @accomodation_photos.each do |accomodation_photo| %>
      <tr>
        <td><%= accomodation_photo.accommodation_id %></td>
        <td><%= accomodation_photo.user_id %></td>
        <td><%= accomodation_photo.caption %></td>
        <td>
          <a href="/accomodation_photos/<%= accomodation_photo.id %>" class="btn btn-primary">Show</a>
          <a href="/accomodation_photos/<%= accomodation_photo.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_accomodation_photo/<%= accomodation_photo.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
